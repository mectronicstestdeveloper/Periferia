    <script type="text/javascript">
        function error_handler(e) {
            if (e.errors) {
                var message = "Errors:\n";
                $.each(e.errors, function (key, value) {
                    if ('errors' in value) {
                        $.each(value.errors, function () {
                            message += this + "\n";
                        });
                    }
                });
                alert(message);
            }
        }
    </script>
    <script type="text/javascript">
        //Globalize.loadMessages(dictionary);
        Globalize.locale("es");

        function Proxy(Verb, Url, ContentType, Data, fxSuccess, fxError, DataType) {

            $.ajax({
                url: Url,
                method: Verb,
                contentType: ContentType,
                data: Data,
                dataType: DataType == null ? "" : DataType,
                statusCode: {
                    403: function () {
                        alert("Espere un momento por favor, vuelva a intentar en unos pocos segundo");
                    },
                    404: function () {
                        alert("Verificar el recurso solicitado");
                    },
                    500: function () {
                        alert("server error");
                    }
                },
                success: function (data) {
                    fxSuccess(data);
                    noProcess();
                },
                error: fxError
            });
        }
    </script>

